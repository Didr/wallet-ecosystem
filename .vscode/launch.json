{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach",
			"port": 9259,
			"restart": true,
			"localRoot": "${workspaceFolder}/wallet-enterprise/src",
			"remoteRoot": "/app/dist/src",
			"sourceMaps": true,
			"resolveSourceMapLocations": [
				"${workspaceFolder}/**",
				"!**/node_modules/**"
			],
			"autoAttachChildProcesses": true
		}
	]
}